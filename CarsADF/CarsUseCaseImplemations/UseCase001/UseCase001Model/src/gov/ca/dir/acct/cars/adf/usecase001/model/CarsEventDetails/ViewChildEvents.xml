  <SQLQuery>
    <![CDATA[SELECT 
    EVENT_TYPE_CODE, 
    EVENT_TYPE_NAME, 
    TRUNC(EVENT_DATE) EVENT_DATE, 
    CURRENT_DOCUMENT, 
    REFERENCE_DOCUMENT, 
    ADDITIONAL_REFERENCE, 
    EVENT.DATA_SOURCE_CODE, 
    AMOUNT, 
    ACCOUNTING_TRANSACTION.ACCTG_TRANSACTION_ID, 
    BILL_TYPE_CODE, 
    RECEIPT_TYPE_CODE, 
    DEPOSIT_SLIP_NUMBER, 
    TRUNC(DEPOSIT_DATE) DEPOSIT_DATE, 
    RECEIPT_CONTROL_NUMBER, 
    TRUNC(RECEIPT_DATE) RECEIPT_DATE, 
    TRUNC(RECEIVABLE_ESTABLISH_DATE) RECEIVABLE_ESTABLISH_DATE, 
    TRUNC(DUE_DATE) DUE_DATE, 
    ADJUSTMENT_TYPE_CODE, 
    TRUNC(ADJUSTMENT_DATE) ADJUSTMENT_DATE, 
    OTHER_TRANSACT_TYPE_CODE, 
    TRUNC(OTHER_TRANSACTION_DATE) OTHER_TRANSACTION_DATE, 
    EVENT.EVENT_ID, 
    EVENT_TYPE.PROGRAM_UNIT_CODE, 
    RECEIPT.OFFSET_PAYMENT_CODE, 
    TRUNC(EVENT.CREATED_DATE) CREATED_DATE 
FROM 
    (((((((((EVENT INNER JOIN EVENT_TYPE ON EVENT.EVENT_TYPE_ID = EVENT_TYPE.EVENT_TYPE_ID) LEFT OUTER JOIN ACCTG_TRANSACT_EVENT_ASSOC ON EVENT.EVENT_ID = ACCTG_TRANSACT_EVENT_ASSOC.EVENT_ID) LEFT OUTER JOIN ACCOUNTING_TRANSACTION ON ACCOUNTING_TRANSACTION.ACCTG_TRANSACTION_ID = ACCTG_TRANSACT_EVENT_ASSOC.ACCTG_TRANSACTION_ID) LEFT OUTER JOIN ADJUSTMENT ON ADJUSTMENT.ACCTG_TRANSACTION_ID = ACCOUNTING_TRANSACTION.ACCTG_TRANSACTION_ID) LEFT OUTER JOIN RECEIVABLE ON RECEIVABLE.ACCTG_TRANSACTION_ID = ACCOUNTING_TRANSACTION.ACCTG_TRANSACTION_ID) LEFT OUTER JOIN RECEIPT ON RECEIPT.ACCTG_TRANSACTION_ID = ACCOUNTING_TRANSACTION.ACCTG_TRANSACTION_ID) LEFT OUTER JOIN OTHER_ACCOUNT_TRANS ON OTHER_ACCOUNT_TRANS.ACCTG_TRANSACTION_ID = ACCOUNTING_TRANSACTION.ACCTG_TRANSACTION_ID) LEFT OUTER JOIN PARTICIPANT_ROLE ON EVENT.EVENT_ID = PARTICIPANT_ROLE.EVENT_ID) LEFT OUTER JOIN ORGANIZATION ON PARTICIPANT_ROLE.PARTY_ID = ORGANIZATION.PARTY_ID) LEFT OUTER JOIN PERSON ON PARTICIPANT_ROLE.PARTY_ID = PERSON.PARTY_ID 
WHERE 
    (PARTICIPANT_ROLE_TYPE_CODE = 'RES_PARTY' OR PARTICIPANT_ROLE_TYPE_CODE IS NULL OR PARTICIPANT_ROLE_TYPE_CODE = 'PAYOR') AND EVENT_TYPE.EVENT_TYPE_CODE NOT IN ('010', '015', '020') 
ORDER BY 
    EVENT_DATE ASC]]>
  </SQLQuery>
