  </ListBinding>
  <SQLQuery>
    <![CDATA[select A.EMPLOYER_NAME,
       A.INVOICE_NUMBER,       
       A.ISSUE_DATE,
       A.FISCAL_YEAR,
       A.DEPOSIT_SLIP_NUMBER,
       A.REFERENCE_NO,
       A.AGENCY_SOURCE_CODE,
       A.REVENUE_SOURCE_CODE,
       A.PCA_CODE,
       A.FUND,
       A.PROGRAM_UNIT_CODE, 
       A.DATA_SOURCE_CODE,
       sum(a.BILLED_ADJUSTMENT_AMOUNT) BILLED_ADJUSTMENT_AMOUNT,
       sum(a.UNBILLED_ADJUSTMENT_AMOUNT) UNBILLED_ADJUSTMENT_AMOUNT,
        --A.INVOICE_AMOUNT,
       SUM(A.COURT_FEE) COURT_FEE,
       SUM(A.JUDGMENT_INTEREST) JUDGMENT_INTEREST ,
       SUM(A.COLLECTION_FEE) COLLECTION_FEE,
       SUM(A.UNAPPLIED) UNAPPLIED,       
       SUM(A.NSF_FEE) NSF_FEE,
SUM(A.POST_JUD_INTEREST) POST_JUD_INTEREST,
       SUM(A.BILLED_AMOUNT) BILLED_AMOUNT,       
       SUM(A.PAYMENT_AMOUNT) PAYMENT_AMOUNT,        
       SUM(A.UNBILLED_AMOUNT) UNBILLED_AMOUNT,       
       A.RECEIPT_DATE,       
       A.DEPOSIT_DATE,   
       A.BATCH_NUMBER,
    a.BATCH_DATE,  
       A.INDEX_CODE
from(
SELECT DISTINCT
       NVL (LEGAL_NAME, FIRST_NAME || ' ' || MIDDLE_NAME || ' ' || LAST_NAME) EMPLOYER_NAME,
       E.AR_ROOT_DOCUMENT INVOICE_NUMBER,       
       FUNC_GET_ISSUE_DATE (E.AR_ROOT_DOCUMENT) ISSUE_DATE,
       FUNC_GET_FISCAL_YEAR (E.AR_ROOT_DOCUMENT) FISCAL_YEAR, BT.ACCTG_ENTRY_ID,
       R.DEPOSIT_SLIP_NUMBER,
       R.RECEIPT_CONTROL_NUMBER REFERENCE_NO,
       CASE program_unit_code
          WHEN 'CALOSHA' THEN ''
          ELSE AC.AGENCY_SOURCE_CODE
       END
          AGENCY_SOURCE_CODE,
       CASE program_unit_code
          WHEN 'CALOSHA' THEN ''
          ELSE AC.REVENUE_SOURCE_CODE
       END
          REVENUE_SOURCE_CODE,
       CASE program_unit_code
          WHEN 'CALOSHA' THEN ''
          ELSE FUND_ALLOCATION.PCA_CODE
       END
          PCA_CODE,
       CASE program_unit_code 
       WHEN 'CALOSHA' THEN '' 
       ELSE FUND.FUND 
       END 
          FUND,
       R.DATA_SOURCE_CODE,
       ET.PROGRAM_UNIT_CODE,       
       
     --  FUNC_GET_CURRENT_BALANCE(E.AR_ROOT_DOCUMENT,E.EVENT_DATE,MIN_DATES.MIN_POSTING_DATE) INVOICE_AMOUNT,
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE', 'EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COURT_FEE ,
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', 'EFT',AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) JUDGMENT_INTEREST ,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) POST_JUD_INTEREST,
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE', 'EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COLLECTION_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   UNAPPLIED,       
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) NSF_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) BILLED_AMOUNT,
       
       -(FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +         
       FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED', 'EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER)) BILLED_ADJUSTMENT_AMOUNT,
       -FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) UNBILLED_ADJUSTMENT_AMOUNT,
              
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', 'EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)    +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) -
       
       
        FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) -

FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) -
              
        FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
        
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','EFT',  AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) PAYMENT_AMOUNT,  
       
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', 'EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)    +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) UNBILLED_AMOUNT,       
      
       TRUNC (R.RECEIPT_DATE) RECEIPT_DATE,
       R.DEPOSIT_DATE,
      
       BATCH_NUMBER,
 to_date(BATCH_DATE , 'yyyy-mm-dd')  BATCH_DATE,
       AC.INDEX_CODE
  FROM receipt R
       INNER JOIN accounting_entry AE
          ON AE.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
       INNER JOIN accounting_code AC
          ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
       LEFT JOIN FUND_ALLOCATION
          ON (AC.FUND_ALLOCATION_ID = FUND_ALLOCATION.FUND_ALLOCATION_ID)
       LEFT JOIN FUND ON (FUND_ALLOCATION.FUND_to_ID = FUND.FUND_ID)
       INNER JOIN acctg_transact_event_assoc ATES
          ON ATES.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
       INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
       INNER JOIN EVENT_TYPE ET ON (E.EVENT_TYPE_ID = ET.EVENT_TYPE_ID)
       INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
       LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
       LEFT JOIN PERSON P ON P.PARTY_ID = PR.PARTY_ID
       
       LEFT JOIN batch_transaction BT
          ON AE.ACCTG_ENTRY_ID = BT.ACCTG_ENTRY_ID


 WHERE RECEIPT_TYPE_CODE = 'EFT'
 
 UNION 
 SELECT DISTINCT
       NVL (LEGAL_NAME, FIRST_NAME || ' ' || MIDDLE_NAME || ' ' || LAST_NAME) EMPLOYER_NAME,
       E.AR_ROOT_DOCUMENT INVOICE_NUMBER,       
       FUNC_GET_ISSUE_DATE (E.AR_ROOT_DOCUMENT) ISSUE_DATE,
       FUNC_GET_FISCAL_YEAR (E.AR_ROOT_DOCUMENT) FISCAL_YEAR, BT.ACCTG_ENTRY_ID,
       R.DEPOSIT_SLIP_NUMBER,
       R.RECEIPT_CONTROL_NUMBER REFERENCE_NO,
       CASE program_unit_code
          WHEN 'CALOSHA' THEN ''
          ELSE AC.AGENCY_SOURCE_CODE
       END
          AGENCY_SOURCE_CODE,
       CASE program_unit_code
          WHEN 'CALOSHA' THEN ''
          ELSE AC.REVENUE_SOURCE_CODE
       END
          REVENUE_SOURCE_CODE,
       CASE program_unit_code
          WHEN 'CALOSHA' THEN ''
          ELSE FUND_ALLOCATION.PCA_CODE
       END
          PCA_CODE,
       CASE program_unit_code 
       WHEN 'CALOSHA' THEN '' 
       ELSE FUND.FUND 
       END 
          FUND,
       R.DATA_SOURCE_CODE,
       ET.PROGRAM_UNIT_CODE,       
       
     --  FUNC_GET_CURRENT_BALANCE(E.AR_ROOT_DOCUMENT,E.EVENT_DATE,MIN_DATES.MIN_POSTING_DATE) INVOICE_AMOUNT,
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE', 'EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COURT_FEE ,
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', 'EFT',AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) JUDGMENT_INTEREST ,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) POST_JUD_INTEREST,
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE', 'EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COLLECTION_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   UNAPPLIED,       
       FUNC_GET_UNBILLED_AMT_AE (E.AR_ROOT_DOCUMENT, TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) NSF_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) BILLED_AMOUNT,
       
-(FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +         FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED', 'EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER)) BILLED_ADJUSTMENT_AMOUNT,
              
        -FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) UNBILLED_ADJUSTMENT_AMOUNT,
              
              FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', 'EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)    +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) -
       
       
        FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) -

FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) -
              
        FUNC_GET_EFT_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
        
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','EFT',  AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) PAYMENT_AMOUNT,  
       
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', 'EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)    +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','EFT', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','EFT', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) UNBILLED_AMOUNT,       
      
       TRUNC (R.RECEIPT_DATE) RECEIPT_DATE,
       R.DEPOSIT_DATE,
      
       BATCH_NUMBER,
 to_date(BATCH_DATE , 'yyyy-mm-dd')  BATCH_DATE,
       AC.INDEX_CODE
  FROM receipt R
    inner join accounting_transaction_assoc  on R.ACCTG_TRANSACTION_ID =
    accounting_transaction_assoc.ACCTG_TRANSACTION_FROM_ID
    INNER JOIN acctg_transact_event_assoc ATES
   ON ATES.ACCTG_TRANSACTION_ID =ACCOUNTING_TRANSACTION_ASSOC.ACCTG_TRANSACTION_TO_ID
   INNER JOIN accounting_entry AE
          ON  ATES.ACCTG_TRANSACTION_ID =AE.ACCTG_TRANSACTION_ID
         INNER JOIN accounting_code AC
          ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
       LEFT JOIN FUND_ALLOCATION
          ON (AC.FUND_ALLOCATION_ID = FUND_ALLOCATION.FUND_ALLOCATION_ID)
       LEFT JOIN FUND ON (FUND_ALLOCATION.FUND_to_ID = FUND.FUND_ID)
      INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
       INNER JOIN EVENT_TYPE ET ON (E.EVENT_TYPE_ID = ET.EVENT_TYPE_ID) 
       INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
       LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
       LEFT JOIN PERSON P ON P.PARTY_ID = PR.PARTY_ID
   LEFT JOIN batch_transaction BT
          ON AE.ACCTG_ENTRY_ID = BT.ACCTG_ENTRY_ID

    
WHERE RECEIPT_TYPE_CODE = 'EFT'
 ) A
 GROUP BY A.EMPLOYER_NAME,
       A.INVOICE_NUMBER,       
       A.ISSUE_DATE,
       A.FISCAL_YEAR,
       A.DEPOSIT_SLIP_NUMBER,
       A.REFERENCE_NO,
       A.AGENCY_SOURCE_CODE,
       A.REVENUE_SOURCE_CODE,
       A.PCA_CODE,
       A.FUND,
       A.DATA_SOURCE_CODE,
       A.PROGRAM_UNIT_CODE,     
       A.RECEIPT_DATE,       
       A.DEPOSIT_DATE,      
       A.BATCH_NUMBER,
       A.BATCH_DATE,
       A.INDEX_CODE]]>
  </SQLQuery>
