  <SQLQuery>
    <![CDATA[select A.EMPLOYER_NAME,
       A.INVOICE_NUMBER,
       A.DEPOSIT_SLIP_NUMBER,
     --  A.RECEIPT_TYPE_CODE,    
       A.AGENCY_SOURCE_CODE,       
       A.REVENUE_SOURCE_CODE,       
       A.PCA_CODE,       
       A.FUND,
       A.PROGRAM_UNIT_CODE,
       A.DATA_SOURCE_CODE,
       A.ISSUE_DATE,
       A.FISCAL_YEAR, 
     --  A.INVOICE_AMOUNT,     
       SUM(A.COURT_FEE) COURT_FEE,
       SUM(A.JUDGMENT_INTEREST) JUDGMENT_INTEREST,   
       SUM(A.COLLECTION_FEE) COLLECTION_FEE,
       SUM(A.UNAPPLIED) UNAPPLIED,
       SUM(A.POST_JUD_INTEREST) POST_JUD_INTEREST,
       SUM(A.NSF_FEE) NSF_FEE ,
       SUM(A.PENALTY_AMOUNT) PENALTY_AMOUNT,
       SUM(A.PAYMENT_AMOUNT) PAYMENT_AMOUNT,
       SUM(A.UNBILLED_AMOUNT) UNBILLED_AMOUNT,
sum(a.BILLED_ADJUSTMENT_AMOUNT) BILLED_ADJUSTMENT_AMOUNT,
       sum(a.UNBILLED_ADJUSTMENT_AMOUNT) UNBILLED_ADJUSTMENT_AMOUNT,
     --  A.event_date, 
       A.RECEIPT_DATE,
       A.CARD_NUMBER,     
       A.DEBT_Date,
       A.BATCH_NUMBER,
A.BATCH_DATE,
       A.INDEX_CODE
FROM

(SELECT DISTINCT
       NVL (LEGAL_NAME, FIRST_NAME || ' ' || MIDDLE_NAME || ' ' || LAST_NAME)
          EMPLOYER_NAME, 
       e.AR_ROOT_DOCUMENT INVOICE_NUMBER,
       R.DEPOSIT_SLIP_NUMBER,
       R.RECEIPT_TYPE_CODE,    
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE AC.AGENCY_SOURCE_CODE
       END AGENCY_SOURCE_CODE,
       
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE AC.REVENUE_SOURCE_CODE
       END REVENUE_SOURCE_CODE,
       
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE  FL.PCA_CODE
       END PCA_CODE,
       
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE  F.FUND
       END FUND,
       ET.PROGRAM_UNIT_CODE,
       E.DATA_SOURCE_CODE,
       FUNC_GET_ISSUE_DATE (e.AR_ROOT_DOCUMENT) ISSUE_DATE,
       FUNC_GET_FISCAL_YEAR (e.AR_ROOT_DOCUMENT) FISCAL_YEAR, BT.ACCTG_ENTRY_ID,
      -- FUNC_GET_CURRENT_BALANCE(E.AR_ROOT_DOCUMENT,E.EVENT_DATE,MIN_DATES.MIN_POSTING_DATE) INVOICE_AMOUNT,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COURT_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', '%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) JUDGMENT_INTEREST,   
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COLLECTION_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   UNAPPLIED,

-(FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) 
+         FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)) BILLED_ADJUSTMENT_AMOUNT,
              
        -FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) UNBILLED_ADJUSTMENT_AMOUNT,

       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) POST_JUD_INTEREST,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) NSF_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED',R.RECEIPT_TYPE_CODE, AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) PENALTY_AMOUNT,
       TRUNC(E.EVENT_DATE) event_date, 
       TRUNC(R.RECEIPT_DATE) RECEIPT_DATE,      
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', '%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) -
       FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) -

FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','%CREDIT_CARD',AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) -
              
        FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED',R.RECEIPT_TYPE_CODE, AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) PAYMENT_AMOUNT,
       
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', '%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD',AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) UNBILLED_AMOUNT,       
 
       R.RECEIPT_CONTROL_NUMBER          CARD_NUMBER,     
       R.DEPOSIT_DATE DEBT_Date,
       BATCH_NUMBER,
to_date(BATCH_DATE, 'yyyy/mm/dd') BATCH_DATE,
       AC.INDEX_CODE
      
       
  FROM receipt R
       INNER JOIN accounting_entry AE
          ON AE.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
       INNER JOIN accounting_code AC
          ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
       INNER JOIN FUND_ALLOCATION FL ON AC.FUND_ALLOCATION_ID = FL.FUND_ALLOCATION_ID
       INNER JOIN FUND F ON FL.FUND_TO_ID = F.FUND_ID
       INNER JOIN acctg_transact_event_assoc ATES
          ON ATES.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
       INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
       INNER JOIN event_type et ON ET.EVENT_TYPE_ID = E.EVENT_TYPE_ID
       INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
       LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
       LEFT JOIN PERSON P ON P.PARTY_ID = PR.PARTY_ID
       LEFT JOIN batch_transaction BT
          ON AE.ACCTG_ENTRY_ID = BT.ACCTG_ENTRY_ID
       

 WHERE RECEIPT_TYPE_CODE IN
          ('VISA_MASTER_CREDIT_CARD',
           'AMEX_CREDIT_CARD',
           'DISCOVER_CREDIT_CARD')
		   
		   UNION 
SELECT DISTINCT
       NVL (LEGAL_NAME, FIRST_NAME || ' ' || MIDDLE_NAME || ' ' || LAST_NAME)
          EMPLOYER_NAME, 
       e.AR_ROOT_DOCUMENT INVOICE_NUMBER,
       R.DEPOSIT_SLIP_NUMBER,
       R.RECEIPT_TYPE_CODE,    
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE AC.AGENCY_SOURCE_CODE
       END AGENCY_SOURCE_CODE,
       
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE AC.REVENUE_SOURCE_CODE
       END REVENUE_SOURCE_CODE,
       
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE  FL.PCA_CODE
       END PCA_CODE,
       
        case program_unit_code
       when 'CALOSHA' THEN ''
       ELSE  F.FUND
       END FUND,
       ET.PROGRAM_UNIT_CODE,
       E.DATA_SOURCE_CODE,
       FUNC_GET_ISSUE_DATE (e.AR_ROOT_DOCUMENT) ISSUE_DATE,
       FUNC_GET_FISCAL_YEAR (e.AR_ROOT_DOCUMENT) FISCAL_YEAR, BT.ACCTG_ENTRY_ID,
      -- FUNC_GET_CURRENT_BALANCE(E.AR_ROOT_DOCUMENT,E.EVENT_DATE,MIN_DATES.MIN_POSTING_DATE) INVOICE_AMOUNT,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COURT_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', '%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) JUDGMENT_INTEREST,   
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) COLLECTION_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   UNAPPLIED,

-(FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) 
+         FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)) BILLED_ADJUSTMENT_AMOUNT,
              
        -FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) UNBILLED_ADJUSTMENT_AMOUNT,

       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) POST_JUD_INTEREST,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) NSF_FEE,
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED',R.RECEIPT_TYPE_CODE, AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) PENALTY_AMOUNT,
       TRUNC(E.EVENT_DATE) event_date, 
       TRUNC(R.RECEIPT_DATE) RECEIPT_DATE,      
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', '%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) -
       FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) -

FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_OTHERS','%CREDIT_CARD',AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) -
              
        FUNC_GET_CC_ADJUSTMENT(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'BILLED',R.RECEIPT_TYPE_CODE, AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) PAYMENT_AMOUNT,
       
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COURT_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_INTEREST', '%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_COLLECTION_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID,R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNAPPLIED','%CREDIT_CARD',AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER)   +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_POST_JUDGMNT_INT','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) +
       FUNC_GET_UNBILLED_AMT_AE(E.AR_ROOT_DOCUMENT,TRUNC(R.RECEIPT_DATE),'UNBILLED_NSF_FEE','%CREDIT_CARD', AE.ACCTG_ENTRY_ID, R.RECEIPT_CONTROL_NUMBER) UNBILLED_AMOUNT,       
 
       R.RECEIPT_CONTROL_NUMBER          CARD_NUMBER,     
       R.DEPOSIT_DATE DEBT_Date,
       BATCH_NUMBER,
to_date(BATCH_DATE, 'yyyy/mm/dd') BATCH_DATE,
       AC.INDEX_CODE
      
       
  FROM receipt R
    inner join accounting_transaction_assoc  on R.ACCTG_TRANSACTION_ID =
    accounting_transaction_assoc.ACCTG_TRANSACTION_FROM_ID
    INNER JOIN acctg_transact_event_assoc ATES
   ON ATES.ACCTG_TRANSACTION_ID =ACCOUNTING_TRANSACTION_ASSOC.ACCTG_TRANSACTION_TO_ID
   INNER JOIN accounting_entry AE
          ON  ATES.ACCTG_TRANSACTION_ID =AE.ACCTG_TRANSACTION_ID
         INNER JOIN accounting_code AC
          ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
       LEFT JOIN FUND_ALLOCATION
          ON (AC.FUND_ALLOCATION_ID = FUND_ALLOCATION.FUND_ALLOCATION_ID)
       LEFT JOIN FUND ON (FUND_ALLOCATION.FUND_to_ID = FUND.FUND_ID)
      INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
       INNER JOIN EVENT_TYPE ET ON (E.EVENT_TYPE_ID = ET.EVENT_TYPE_ID) 
       INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
       LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
       LEFT JOIN PERSON P ON P.PARTY_ID = PR.PARTY_ID
   LEFT JOIN batch_transaction BT
          ON AE.ACCTG_ENTRY_ID = BT.ACCTG_ENTRY_ID
           INNER JOIN FUND_ALLOCATION FL ON AC.FUND_ALLOCATION_ID = FL.FUND_ALLOCATION_ID
       INNER JOIN FUND F ON FL.FUND_TO_ID = F.FUND_ID
       

 WHERE RECEIPT_TYPE_CODE IN
          ('VISA_MASTER_CREDIT_CARD',
           'AMEX_CREDIT_CARD',
           'DISCOVER_CREDIT_CARD')
		   
		   )  a
           
group by A.EMPLOYER_NAME,
       A.INVOICE_NUMBER,
       A.DEPOSIT_SLIP_NUMBER,
     --  A.RECEIPT_TYPE_CODE,    
       A.AGENCY_SOURCE_CODE,       
       A.REVENUE_SOURCE_CODE,       
       A.PCA_CODE,       
       A.FUND,
       A.PROGRAM_UNIT_CODE,
       A.DATA_SOURCE_CODE,
       A.ISSUE_DATE,
       A.FISCAL_YEAR,
     --  A.INVOICE_AMOUNT,        
      -- A.event_date, 
       A.RECEIPT_DATE,
       A.CARD_NUMBER,     
       A.DEBT_Date,
       A.BATCH_NUMBER,
A.BATCH_DATE,
       A.INDEX_CODE]]>
  </SQLQuery>
