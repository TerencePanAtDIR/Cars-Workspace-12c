  <SQLQuery>
    <![CDATA[SELECT checks.employerName,
         checks.INVOICE_NUMBER,
         checks.ISSUE_DATE,
         checks.FISCAL_YEAR,
         checks.DEPOSIT_DATE,
         checks.DEPOSIT_SLIP_NUMBER,
         checks.UNAPPLIED,
         checks.BILLED_AMOUNT,
         checks.BILLED_ADJUSTMENT,
         checks.UNBILLED_ADJUSTMENT,
         checks.COURT_FEE,
         checks.INTEREST JUDGEMENT_INTEREST,
         checks.POST_JUD_INTEREST,
         checks.NSF NSF_FEE,
         checks.COLLECTION_FEE,
         checks.PAYMENT_AMOUNT,
         CHECKS.CHECK_NUMBER,
         CHECKS.RECEIPT_DATE,
         checks.OFFSET_PAYMENT_CODE           AS ATTRIBUTE_CODE,
         checks.DEPOSIT_ID,
         checks.ACCOUNTING_DT,
         CHECKS.INDEX_CODE,
         checks.PCA_CODE,
         checks.REVENUE_SOURCE_CODE,
         checks.AGENCY_SOURCE_CODE,
         checks.FUND,
         checks.PROGRAM_UNIT_CODE,
         checks.DATA_SOURCE_CODE
    FROM (SELECT 
                 LEGAL_NAME employerName,
                 E.AR_ROOT_DOCUMENT
                     INVOICE_NUMBER,
                 R.DEPOSIT_SLIP_NUMBER,
                 R.OFFSET_PAYMENT_CODE,
                 AC.AGENCY_SOURCE_CODE,
                 AC.REVENUE_SOURCE_CODE,
                 FUND_ALLOCATION.PCA_CODE,
                 FUND.FUND,
                 R.DATA_SOURCE_CODE,
                 ET.PROGRAM_UNIT_CODE,
                 FUNC_GET_ISSUE_DATE (E.AR_ROOT_DOCUMENT)
                     ISSUE_DATE,
                 FUNC_GET_FISCAL_YEAR (E.AR_ROOT_DOCUMENT)
                     FISCAL_YEAR,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_COURT_FEE' THEN AE.AMOUNT
                        ELSE 0
                     END
                     COURT_FEE,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_INTEREST' THEN AE.AMOUNT
                        ELSE 0
                     END
                     INTEREST,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_COLLECTION_FEE' THEN AE.AMOUNT
                        ELSE 0
                     END
                     COLLECTION_FEE,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNAPPLIED' THEN AE.AMOUNT
                        ELSE 0
                     END
                     UNAPPLIED,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_POST_JUDGMNT_INT' THEN AE.AMOUNT
                        ELSE 0
                     END
                     POST_JUD_INTEREST,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_NSF_FEE' THEN AE.AMOUNT
                        ELSE 0
                     END
                     NSF,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'BILLED' THEN AE.AMOUNT
                        ELSE 0
                     END
                     BILLED_AMOUNT,
                    0
                     BILLED_ADJUSTMENT,
                    0
                     UNBILLED_ADJUSTMENT,
                 TRUNC (EVENT_DATE),
                 TRUNC (R.RECEIPT_DATE)
                     RECEIPT_DATE,
                 R.RECEIPT_CONTROL_NUMBER
                     CHECK_NUMBER,
                   AE.AMOUNT
                     PAYMENT_AMOUNT,
                   CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_COURT_FEE' THEN AE.AMOUNT
                        ELSE 0
                     END
                 + 
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_INTEREST' THEN AE.AMOUNT
                        ELSE 0
                     END
                 + 
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_COLLECTION_FEE' THEN AE.AMOUNT
                        ELSE 0
                     END
                 + 
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNAPPLIED' THEN AE.AMOUNT
                        ELSE 0
                     END
                 + 
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_POST_JUDGMNT_INT' THEN AE.AMOUNT
                        ELSE 0
                     END
                 + 
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_NSF_FEE' THEN AE.AMOUNT
                        ELSE 0
                     END
                     UNBILLED_AMOUNT,
                 TRUNC (R.DEPOSIT_DATE)
                     DEPOSIT_DATE,
                 AC.INDEX_CODE,
                 IF001.DEPOSIT_ID,
                 IF001.ACCOUNTING_DT
            FROM receipt R
                 INNER JOIN accounting_entry AE
                     ON AE.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
                 INNER JOIN accounting_entry_type AET
                     ON AE.ACCTG_ENTRY_TYPE_ID = AET.ACCTG_ENTRY_TYPE_ID
                     AND AET.ACCTG_TRANSACT_CODE IN ('101', '142')
                 INNER JOIN accounting_code AC
                     ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
                 LEFT JOIN FUND_ALLOCATION
                     ON (AC.FUND_ALLOCATION_ID =
                         FUND_ALLOCATION.FUND_ALLOCATION_ID)
                 LEFT JOIN FUND ON (FUND_ALLOCATION.FUND_to_ID = FUND.FUND_ID)
                 INNER JOIN acctg_transact_event_assoc ATES
                     ON ATES.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
                 INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
                 INNER JOIN EVENT_TYPE ET
                     ON (E.EVENT_TYPE_ID = ET.EVENT_TYPE_ID)
                 INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
                 LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
                 LEFT JOIN
                 (SELECT ACCTG_ENTRY_ID, DEPOSIT_ID, ACCOUNTING_DT
                    FROM INFAR001_OUTBOUND
                   WHERE     FS_ROW_ID = '002'
                         AND STATUS = 'TRANSMITTED'
                         AND PAYMENT_ID <> 'OFFSET') IF001
                     ON IF001.ACCTG_ENTRY_ID = AE.ACCTG_ENTRY_ID
           WHERE     R.RECEIPT_TYPE_CODE IN ('CHECK',
                                             'EDF',
                                             'USB_LOCK_BOX',
                                             'EDF_REMOTE',
                                             'EDF_STANDARD',
                                             'JPMC_LOCK_BOX')
          UNION ALL
          SELECT 
                 LEGAL_NAME employerName,
                 E.AR_ROOT_DOCUMENT
                     INVOICE_NUMBER,
                 R.DEPOSIT_SLIP_NUMBER,
                 RR.OFFSET_PAYMENT_CODE,
                 AC.AGENCY_SOURCE_CODE,
                 AC.REVENUE_SOURCE_CODE,
                 FUND_ALLOCATION.PCA_CODE,
                 FUND.FUND,
                 R.DATA_SOURCE_CODE,
                 ET.PROGRAM_UNIT_CODE,
                 FUNC_GET_ISSUE_DATE (E.AR_ROOT_DOCUMENT)
                     ISSUE_DATE,
                 FUNC_GET_FISCAL_YEAR (E.AR_ROOT_DOCUMENT)
                     FISCAL_YEAR,
                     0
                     COURT_FEE,
                     0
                     INTEREST,
                     0
                     COLLECTION_FEE,
                     0
                     UNAPPLIED,
                     0
                     POST_JUD_INTEREST,
                     0
                     NSF,
                     0
                     BILLED_AMOUNT,
                 -(  CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_OTHERS' THEN AE.AMOUNT
                        ELSE 0
                     END
                   + CASE R.BILL_TYPE_CODE
                        WHEN 'BILLED' THEN AE.AMOUNT
                        ELSE 0
                     END)
                     BILLED_ADJUSTMENT,
                 -CASE R.BILL_TYPE_CODE
                        WHEN 'UNAPPLIED' THEN AE.AMOUNT
                        ELSE 0
                     END
                     UNBILLED_ADJUSTMENT,
                 TRUNC (EVENT_DATE),
                 TRUNC (R.RECEIPT_DATE)
                     RECEIPT_DATE,
                 R.RECEIPT_CONTROL_NUMBER
                     CHECK_NUMBER,
                 - CASE R.BILL_TYPE_CODE
                        WHEN 'BILLED' THEN AE.AMOUNT
                        ELSE 0
                     END
                 - CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_OTHERS' THEN AE.AMOUNT
                        ELSE 0
                     END
                 - CASE R.BILL_TYPE_CODE
                        WHEN 'UNAPPLIED' THEN AE.AMOUNT
                        ELSE 0
                     END
                     PAYMENT_AMOUNT,
                     0
                     UNBILLED_AMOUNT,
                 TRUNC (R.DEPOSIT_DATE)
                     DEPOSIT_DATE,
                 AC.INDEX_CODE,
                 IF001.DEPOSIT_ID,
                 IF001.ACCOUNTING_DT
            FROM receipt R
                 INNER JOIN accounting_transaction_assoc ata
                     ON r.ACCTG_TRANSACTION_ID = ata.ACCTG_TRANSACTION_FROM_ID
                 --Reversal event's receipt
                 INNER JOIN receipt RR
                     ON RR.ACCTG_TRANSACTION_ID = ata.ACCTG_TRANSACTION_TO_ID
                 INNER JOIN acctg_transact_event_assoc ATES
                     ON ATES.ACCTG_TRANSACTION_ID = ata.ACCTG_TRANSACTION_TO_ID
                 INNER JOIN accounting_entry AE
                     ON ATES.ACCTG_TRANSACTION_ID = AE.ACCTG_TRANSACTION_ID
                 INNER JOIN accounting_entry_type AET
                     ON AE.ACCTG_ENTRY_TYPE_ID = AET.ACCTG_ENTRY_TYPE_ID
                     AND AET.ACCTG_TRANSACT_CODE IN ('101', '142')
                 INNER JOIN accounting_code AC
                     ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
                 LEFT JOIN FUND_ALLOCATION
                     ON (AC.FUND_ALLOCATION_ID =
                         FUND_ALLOCATION.FUND_ALLOCATION_ID)
                 LEFT JOIN FUND ON (FUND_ALLOCATION.FUND_to_ID = FUND.FUND_ID)
                 INNER JOIN acctg_transact_event_assoc ATES
                     ON ATES.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
                 INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
                 INNER JOIN EVENT_TYPE ET
                     ON (E.EVENT_TYPE_ID = ET.EVENT_TYPE_ID)
                 INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
                 LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
                 LEFT JOIN
                 (SELECT ACCTG_ENTRY_ID, DEPOSIT_ID, ACCOUNTING_DT
                    FROM INFAR001_OUTBOUND
                   WHERE     FS_ROW_ID = '002'
                         AND STATUS = 'TRANSMITTED'
                         AND PAYMENT_ID <> 'OFFSET') IF001
                     ON IF001.ACCTG_ENTRY_ID = AE.ACCTG_ENTRY_ID
           WHERE     R.RECEIPT_TYPE_CODE IN ('CHECK',
                                             'EDF',
                                             'USB_LOCK_BOX',
                                             'EDF_REMOTE',
                                             'EDF_STANDARD',
                                             'JPMC_LOCK_BOX')
        UNION ALL
        SELECT 
                 LEGAL_NAME employerName,
                 E.AR_ROOT_DOCUMENT
                     INVOICE_NUMBER,
                 R.DEPOSIT_SLIP_NUMBER,
                 R.OFFSET_PAYMENT_CODE,
                 AC.AGENCY_SOURCE_CODE,
                 AC.REVENUE_SOURCE_CODE,
                 FUND_ALLOCATION.PCA_CODE,
                 FUND.FUND,
                 R.DATA_SOURCE_CODE,
                 ET.PROGRAM_UNIT_CODE,
                 FUNC_GET_ISSUE_DATE (E.AR_ROOT_DOCUMENT)
                     ISSUE_DATE,
                 FUNC_GET_FISCAL_YEAR (E.AR_ROOT_DOCUMENT)
                     FISCAL_YEAR,
                     0
                     COURT_FEE,
                     CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_INTEREST' THEN AE.AMOUNT
                        ELSE 0
                     END
                     INTEREST,
                     0
                     COLLECTION_FEE,
                     0
                     UNAPPLIED,
                     0
                     POST_JUD_INTEREST,
                     0
                     NSF,
                     0
                     BILLED_AMOUNT,
                 -CASE R.BILL_TYPE_CODE
                        WHEN 'BILLED' THEN AE.AMOUNT
                        ELSE 0
                     END
                 -CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_OTHERS' THEN AE.AMOUNT
                        ELSE 0
                     END
                     BILLED_ADJUSTMENT,
                 -CASE R.BILL_TYPE_CODE
                        WHEN 'UNAPPLIED' THEN AE.AMOUNT
                        ELSE 0
                     END
                     UNBILLED_ADJUSTMENT,
                 TRUNC (EVENT_DATE),
                 TRUNC (R.RECEIPT_DATE)
                     RECEIPT_DATE,
                 R.RECEIPT_CONTROL_NUMBER
                     CHECK_NUMBER,
                 - CASE R.BILL_TYPE_CODE
                        WHEN 'BILLED' THEN AE.AMOUNT
                        ELSE 0
                     END
                 - CASE R.BILL_TYPE_CODE
                        WHEN 'UNBILLED_OTHERS' THEN AE.AMOUNT
                        ELSE 0
                     END
                 - CASE R.BILL_TYPE_CODE
                        WHEN 'UNAPPLIED' THEN AE.AMOUNT
                        ELSE 0
                     END
                     PAYMENT_AMOUNT,
                     0
                     UNBILLED_AMOUNT,
                 TRUNC (R.DEPOSIT_DATE)
                     DEPOSIT_DATE,
                 AC.INDEX_CODE,
                 IF001.DEPOSIT_ID,
                 IF001.ACCOUNTING_DT
            FROM receipt R
                 INNER JOIN accounting_entry AE
                     ON AE.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
                 INNER JOIN accounting_entry_type AET
                     ON AE.ACCTG_ENTRY_TYPE_ID = AET.ACCTG_ENTRY_TYPE_ID
                 INNER JOIN accounting_code AC
                     ON AC.ACCOUNTING_CODE_ID = AE.ACCOUNTING_CODE_ID
                 LEFT JOIN FUND_ALLOCATION
                     ON (AC.FUND_ALLOCATION_ID =
                         FUND_ALLOCATION.FUND_ALLOCATION_ID)
                 LEFT JOIN FUND ON (FUND_ALLOCATION.FUND_to_ID = FUND.FUND_ID)
                 INNER JOIN acctg_transact_event_assoc ATES
                     ON ATES.ACCTG_TRANSACTION_ID = R.ACCTG_TRANSACTION_ID
                 INNER JOIN EVENT E ON E.EVENT_ID = ATES.EVENT_ID
                 INNER JOIN EVENT_TYPE ET
                     ON (E.EVENT_TYPE_ID = ET.EVENT_TYPE_ID)
                 INNER JOIN participant_role PR ON PR.EVENT_ID = E.EVENT_ID
                 LEFT JOIN organization O ON O.PARTY_ID = PR.PARTY_ID
                 LEFT JOIN
                 (SELECT ACCTG_ENTRY_ID, DEPOSIT_ID, ACCOUNTING_DT
                    FROM INFAR001_OUTBOUND
                   WHERE     FS_ROW_ID = '002'
                         AND STATUS = 'TRANSMITTED'
                         AND PAYMENT_ID <> 'OFFSET') IF001
                     ON IF001.ACCTG_ENTRY_ID = AE.ACCTG_ENTRY_ID
           WHERE    EVENT_TYPE_CODE IN ('343', '344') 
               AND  (R.DEPOSIT_SLIP_NUMBER LIKE 'DS%'
                or  R.DEPOSIT_SLIP_NUMBER LIKE 'BX%')
                                             ) checks]]>
  </SQLQuery>
